{
    "manifest_version": 3,
    "name": "Tax Calculator Extension",
    "version": "1.0",
    "permissions": ["activeTab", "storage"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [{
      "matches": ["*://*.incometax.gov.in/*"],
      "js": ["content.js"]
    }]
  }
  